{
  "name": "voyage-discord-bot",
  "version": "0.9.7",
  "description": "Discord Bot made with NestJS and discord-nestjs",
  "private": true,
  "engines": {
    "node": "^16.0.0",
    "npm": "^8.1.0"
  },
  "scripts": {
    "init": "gulp init",
    "build": "gulp build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:debug:no-watch": "nest start --debug",
    "start:prod": "node ./dist/main.js",
    "clean:db": "gulp cleanDb",
    "delete:dist": "gulp deleteDist",
    "generate": "gulp setupPrisma",
    "lint": "npm-run-all lint:*",
    "lint:src": "eslint \"{src,apps,libs}/**/*.ts\"",
    "lint:tests": "eslint \"tests/**/*.ts\"",
    "format": "npm-run-all format:*",
    "format:prettier": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:eslint": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix",
    "format:prisma": "prisma format",
    "test": "vitest --config tests/.vitest/vitest.all-config.ts",
    "test:specs": "vitest --config tests/.vitest/vitest.specs-config.ts",
    "test:e2e": "vitest --config tests/.vitest/vitest.e2e-config.ts",
    "test:cov": "pnpm test --coverage",
    "seed": "gulp seed",
    "studio": "prisma studio"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@discord-nestjs/common": "3.0.7",
    "@discord-nestjs/core": "3.0.7",
    "@discordjs/builders": "0.12.0",
    "@discordjs/opus": "0.7.0",
    "@discordjs/voice": "0.8.0",
    "@nestjs/apollo": "10.0.8",
    "@nestjs/common": "8.4.4",
    "@nestjs/core": "8.4.4",
    "@nestjs/graphql": "10.0.8",
    "@nestjs/platform-express": "8.4.4",
    "@paljs/plugins": "4.0.18",
    "@prisma/client": "3.12.0",
    "apollo-server-core": "3.6.7",
    "apollo-server-express": "3.6.7",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.2",
    "deepmerge": "4.2.2",
    "discord-music-player": "github:V-ed/discord-music-player",
    "discord.js": "13.6.0",
    "dotenv": "16.0.0",
    "dotenv-expand": "8.0.3",
    "ffmpeg": "0.0.4",
    "ffmpeg-static": "4.4.1",
    "graphql": "16.3.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-ws": "5.7.0",
    "nest-typed-config": "2.4.1",
    "nestjs-dynamic-providers": "0.1.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.5",
    "ytdl-core": "4.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "6.1.2",
    "@nestjs/cli": "8.2.5",
    "@nestjs/schematics": "8.0.11",
    "@nestjs/testing": "8.4.4",
    "@swc/core": "1.2.185",
    "@types/express": "4.17.13",
    "@types/graphql-depth-limit": "1.1.3",
    "@types/gulp": "4.0.9",
    "@types/gulp-rename": "2.0.1",
    "@types/gulp-replace": "0.0.31",
    "@types/supertest": "2.0.12",
    "@v-ed/tsconfig": "0.2.0",
    "c8": "7.11.3",
    "del": "6.0.0",
    "graphql-config": "4.3.1",
    "graphql-tag": "2.12.6",
    "gulp": "4.0.2",
    "gulp-prompt": "1.2.0",
    "gulp-rename": "2.0.0",
    "gulp-replace": "1.1.3",
    "gulp-typescript": "6.0.0-alpha.1",
    "prisma": "3.12.0",
    "prisma-fixtures": "0.1.5",
    "prisma-nestjs-graphql": "15.2.1",
    "supertest": "6.2.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.3",
    "unplugin-swc": "1.3.2",
    "vite-tsconfig-paths": "3.4.1",
    "vitest": "0.12.2"
  }
}
