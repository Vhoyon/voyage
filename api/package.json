{
  "name": "voyage-discord-bot",
  "version": "0.9.9",
  "description": "Discord Bot made with NestJS and discord-nestjs",
  "private": true,
  "engines": {
    "node": "^16.0.0",
    "npm": "^8.1.0"
  },
  "scripts": {
    "init": "gulp init",
    "build": "gulp build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:debug:no-watch": "nest start --debug",
    "start:prod": "node ./dist/main.js",
    "clean:db": "gulp cleanDb",
    "delete:dist": "gulp deleteDist",
    "generate": "gulp setupPrisma",
    "lint": "npm-run-all lint:*",
    "lint:src": "eslint \"{src,apps,libs}/**/*.ts\"",
    "lint:tests": "eslint \"tests/**/*.ts\"",
    "format": "npm-run-all format:*",
    "format:prettier": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:eslint": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix",
    "format:prisma": "prisma format",
    "test": "vitest --config tests/.vitest/vitest.all-config.ts",
    "test:specs": "vitest --config tests/.vitest/vitest.specs-config.ts",
    "test:e2e": "vitest --config tests/.vitest/vitest.e2e-config.ts",
    "test:cov": "pnpm test --coverage",
    "seed": "gulp seed",
    "studio": "prisma studio"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.seed.json prisma/seed.ts"
  },
  "dependencies": {
    "@discord-nestjs/common": "3.4.2",
    "@discord-nestjs/core": "3.6.0",
    "@discordjs/builders": "0.16.0",
    "@discordjs/opus": "0.9.0",
    "@discordjs/voice": "0.16.1",
    "@nestjs/apollo": "10.2.1",
    "@nestjs/common": "9.4.3",
    "@nestjs/core": "9.4.3",
    "@nestjs/graphql": "10.2.1",
    "@nestjs/platform-express": "9.4.3",
    "@paljs/plugins": "4.1.0",
    "@prisma/client": "4.16.2",
    "@prisma/internals": "4.16.2",
    "@apollo/server": "4.9.5",
    "apollo-server-express": "3.13.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.14.1",
    "deepmerge": "4.3.1",
    "discord-music-player": "github:V-ed/discord-music-player",
    "discord.js": "13.6.0",
    "dotenv": "16.3.2",
    "dotenv-expand": "8.0.3",
    "ffmpeg": "0.0.4",
    "ffmpeg-static": "4.4.1",
    "graphql": "16.8.1",
    "graphql-depth-limit": "1.1.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-ws": "5.14.3",
    "nest-typed-config": "2.9.2",
    "nestjs-dynamic-providers": "0.3.4",
    "reflect-metadata": "0.2.1",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "ytdl-core": "4.11.5"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "9.4.3",
    "@swc/core": "1.4.2",
    "@types/express": "4.17.21",
    "@types/graphql-depth-limit": "1.1.6",
    "@types/gulp": "4.0.17",
    "@types/gulp-rename": "2.0.6",
    "@types/supertest": "6.0.2",
    "@v-ed/tsconfig": "0.3.0",
    "c8": "9.1.0",
    "del": "6.1.1",
    "graphql-tag": "2.12.6",
    "gulp": "4.0.2",
    "gulp-prompt": "1.2.0",
    "gulp-rename": "2.0.0",
    "gulp-replace": "1.1.4",
    "gulp-typescript": "6.0.0-alpha.1",
    "prisma": "4.16.2",
    "prisma-fixtures": "0.1.15",
    "prisma-nestjs-graphql": "17.1.0",
    "supertest": "6.3.4",
    "ts-node": "10.9.2",
    "typescript": "4.9.5",
    "unplugin-swc": "1.4.4",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "0.34.6"
  }
}
